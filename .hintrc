{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 107",
    "not firefox <= 109",
    "not ios_saf <= 14.8",
    "not opera <= 92"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "manifest-file-extension": "off",
    "detect-css-reflows/layout": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "no-protocol-relative-urls": "off"
  }
}